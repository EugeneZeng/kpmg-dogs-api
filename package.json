{
  "name": "kpmg-dogs-api",
  "version": "1.0.0",
  "description": "Restful API services for KPMG dogs.",
  "main": "server.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "build": "tsc & docker-compose build web",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "build:run": "yarn build & yarn run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EugeneZeng/kpmg-dogs-api.git"
  },
  "keywords": [
    "Restful",
    "dogs",
    "kpmg"
  ],
  "author": "Eugene Zeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EugeneZeng/kpmg-dogs-api/issues"
  },
  "homepage": "https://github.com/EugeneZeng/kpmg-dogs-api#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.4",
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.1.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
